@model IEnumerable<WebApplication2.Models.titleauthor>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.author.au_lname)
            
        </th>
        <th>
            @Html.DisplayNameFor(model => model.title.title1)
  
        </th>
        <th>
            @Html.DisplayNameFor(model => model.au_ord)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.royaltyper)
        </th>
        <th></th>
    </tr>


    @foreach (var item in Model)
    {
        <tr>
            <td>
               @Html.DisplayFor(modelItem => item.author.au_lname)
            </td>
            <td>
                
                @Html.DisplayFor(modelItem => item.title.title1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.au_ord)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.royaltyper)
            </td>
            <td>
                <a href=@Url.Action("Edit", "Titleauthors", new { title_id = item.title_id, au_id = item.au_id })> Edit |</a>
                <a href=@Url.Action("Details", "Titleauthors", new { au_id = item.au_id, title_id = item.title_id })> Details |</a>
                <a href=@Url.Action("Delete", "Titleauthors", new { title_id = item.title_id, au_id = item.au_id })> Delete</a>
            </td>
        </tr>
    }

</table>
